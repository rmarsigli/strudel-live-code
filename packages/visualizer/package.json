{
  "name": "@strudel-studio/visualizer",
  "version": "1.0.0",
  "description": "Real-time Strudel code visualizer component with intelligent parsing",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "npm run build && node test-runner.mjs",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "strudel",
    "music",
    "visualizer",
    "live-coding",
    "audio",
    "parser",
    "react"
  ],
  "author": "Strudel Studio",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rmarsigli/strudel-studio.git",
    "directory": "packages/visualizer"
  }
}
